{
  "name": "tentwofour/sf2-form",
  "type": "library",
  "description": "Provides data transformers for Symfony2 Forms",
  "keywords": [
    "ten24",
    "tentwofour",
    "symfony",
    "form",
    "datatransformer"
  ],
  "homepage": "https://github.com/tentwofour/sf2-form",
  "license": "MIT",
  "authors": [
    {
      "name": "Blair D. Patterson",
      "homepage": "http://tentwentyfour.ca",
      "email": "blair@tentwentyfour.ca"
    }
  ],
  "require": {
    "php": "^7.0",
    "symfony/form": "^3.0|^4.0",
    "tentwofour/formatter": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Ten24\\Component\\Form\\Extension\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ten24\\Tests\\Component\\Form\\Extension\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.2.x-dev"
    }
  }
}
